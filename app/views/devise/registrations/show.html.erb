<h1>Perfil de <%= @user.email %></h1>
<p>Nombre: <%= @user.name %></p>
<p>Teléfono: <%= @user.phone %></p>
<p>Edad: <%= @user.age %></p>

<% if @user.images.any? %>
  <h2>Imágenes de perfil:</h2>
  <% @user.images.each do |image| %>
    <div>
      <p>URL: <%= image.url %></p>
      <p>Contexto: <%= image.context %></p>
    </div>
  <% end %>
<% end %>